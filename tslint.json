{
  "extends": ["tslint:recommended", "tslint-config-standard"],
  "cliOptions": {
    "exclude": [
      "src/api.ts"
    ]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "trailing-comma": true,
    "import-spacing": false,
    "no-multi-spaces": false,
    "member-ordering": false,
    "typedef-whitespace": false,
    "await-promise": false,
    "arrow-parens": false
  }
}
